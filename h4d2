            self.tab1_container_layout.removeWidget(self.nmap_Scan2)
            self.nmap_Scan2.deleteLater()
        self.nmap_Scan2 = QLabel(f"nmap -p{self.rport} -sC -sV {self.rhost} --open") 
        self.nmap_Scan2.setStyleSheet(
            "color: 'white';" +
