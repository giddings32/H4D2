# wpscanBF
        if hasattr(self, 'wpscanBF'):
            self.tab2_container_layout.removeWidget(self.wpscanBF)
            self.wpscanBF.deleteLater()
        self.wpscanBF = QLabel("[+ WPScan Brute Forcing:]")
        self.wpscanBF.setStyleSheet(
            "color: #20C20E;" +
